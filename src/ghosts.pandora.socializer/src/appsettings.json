{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationConfiguration": {
    "DefaultDisplay": 35,
    "MinutesToCheckForDuplicatePost": 2,
    "CleanupDiskUtilThreshold": 70,
    "CleanupJob": {
      "Hours": 0,
      "Minutes": 15,
      "Seconds": 0
    },
    "CleanupAge": {
      "Days": 0,
      "Hours": 60,
      "Minutes": 0
    }
  }
}
